{
  "name": "evtjsc",
  "description": "Command line tool to interact with everiToken public blockchain",
  "version": "0.1.5",
  "author": "Fei Liu <liu.feiwood@gmail.com>",
  "bin": {
    "evtjsc": "./bin/run"
  },
  "bugs": "https://github.com/geastwood/evtjsc/issues",
  "dependencies": {
    "@binance-chain/javascript-sdk": "^2.15.0",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "chalk": "^2.4.2",
    "csv-parser": "^2.3.0",
    "evtjs": "^5.5.1",
    "lodash": "^4.17.15",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@types/": "binance-chain/javascript-sdk",
    "@types/chai": "^4.2.2",
    "@types/lodash": "^4.14.138",
    "@types/mocha": "^5",
    "@types/node": "^12.7.4",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "chai": "^4",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.3.0",
    "globby": "^10.0.1",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/geastwood/evtjsc",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "evtjsc",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "geastwood/evtjsc",
  "scripts": {
    "lint": "eslint -c ./.eslintrc.js --ext .ts src/**",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "rm -rf lib tsconfig.tsbuildinfo && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "test:watch": "mocha --forbid-only --watch \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
